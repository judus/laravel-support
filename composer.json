{
    "name": "maduser/laravel-support",
    "type": "library",
    "description": "Support classes for Laravel projects",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Maduser\\Laravel\\Support\\": "src/"
        },
        "classmap": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Maduser\\Laravel\\Support\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
